{
  "name": "google-drive-sync",
  "version": "4.1.1",
  "description": "Share your Google Drive files with this daemon to have them saved to disk as JSON.",
  "bin": {
    "google-drive-sync": "lib/cli.js",
    "google-drive-sync-convert": "lib/convert-cli.js"
  },
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel -d lib/ src/",
    "prepare": "npm run compile",
    "test": "mocha -r @babel/register test/*"
  },
  "repository": {
    "type": "git",
    "url": "git@githubcom:jarib/google-drive-sync.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "babel": {
    "presets": [
      "@babel/env"
    ]
  },
  "author": "Jari Bakken <jari.bakken@gmail.com>",
  "license": "BSD-3-Clause",
  "keywords": [
    "google",
    "drive",
    "google-drive",
    "archieml",
    "json",
    "docs",
    "sheets",
    "spreadsheets",
    "excel",
    "xlsx"
  ],
  "dependencies": {
    "archieml": "^0.4.2",
    "aws-sdk": "^2.643.0",
    "bluebird": "^3.7.2",
    "daemon": "^1.1.0",
    "debug": "^4.1.1",
    "fs-extra": "^4.0.3",
    "googleapis": "^100.0.0",
    "html-entities": "^1.2.0",
    "htmlparser2": "^3.10.1",
    "lodash.zip": "^4.0.0",
    "moment": "^2.24.0",
    "ms": "^2.1.2",
    "stream-buffers": "^3.0.2",
    "underscore": "^1.9.2",
    "xlsx": "^0.18.5",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "mocha": "^6.2.2",
    "node-fetch": "^2.6.1"
  }
}
